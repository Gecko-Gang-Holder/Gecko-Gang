{
  "headers": [
    {
      "source": "/(.*)",  // This applies the headers to all pages
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; frame-ancestors 'self' https://gecko-gang.vercel.app; object-src 'none';"
        }
      ]
    }
  ]
}
